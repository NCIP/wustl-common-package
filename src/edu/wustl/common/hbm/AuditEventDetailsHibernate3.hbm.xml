<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.crs.domain">
  <class name="AuditEventDetails" table="crs_audit_event_details" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="IDENTIFIER">
      <generator class="assigned" />
    </id>
    <property name="currentValue" type="java.lang.String" column="CURRENT_VALUE" />
    <property name="elementName" type="java.lang.String" column="ELEMENT_NAME" />
    <property name="previousValue" type="java.lang.String" column="PREVIOUS_VALUE" />
    <many-to-one name="auditEventLog" class="edu.wustl.crs.domain.AuditEventLog" column="AUDIT_EVENT_LOG_ID" lazy="true" />
  </class>
</hibernate-mapping>

