<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.crs.domain">
  <class name="AuditEvent" table="crs_audit_event" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="IDENTIFIER">
      <generator class="assigned" />
    </id>
    <property name="ipAddress" type="java.lang.String" column="IP_ADDRESS" />
    <property name="timeStamp" type="java.util.Date" column="TIME_STAMP" />
    <property name="userId" type="java.lang.Long" column="USER_ID" />
    <set name="auditEventLogCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="AUDIT_EVENT_ID" />
      <one-to-many class="edu.wustl.crs.domain.AuditEventLog" />
    </set>
  </class>
</hibernate-mapping>

