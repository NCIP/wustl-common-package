<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="edu.wustl.crs.domain">
  <class name="AuditEventLog" table="crs_audit_event_log" lazy="true" polymorphism="explicit">
    <cache usage="read-write" />
    <id name="id" type="java.lang.Long" column="IDENTIFIER">
      <generator class="assigned" />
    </id>
    <property name="objectIdentifier" type="java.lang.Long" column="OBJECT_IDENTIFIER" />
    <property name="objectName" type="java.lang.String" column="OBJECT_NAME" />
    <property name="eventType" type="java.lang.String" column="EVENT_TYPE" />
    <set name="auditEventDetailsCollection" lazy="true" inverse="true">
      <cache usage="read-write" />
      <key column="AUDIT_EVENT_LOG_ID" />
      <one-to-many class="edu.wustl.crs.domain.AuditEventDetails" />
    </set>
    <many-to-one name="auditEvent" class="edu.wustl.crs.domain.AuditEvent" column="AUDIT_EVENT_ID" lazy="true" />
  </class>
</hibernate-mapping>

